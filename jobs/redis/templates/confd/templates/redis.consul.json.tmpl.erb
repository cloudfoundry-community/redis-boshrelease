{
    "service": {
        "name": "<%= p("consul.service.name", spec.deployment) %>",
        "tags": ["{{ .role }}"],
        "port": <%= p("redis.port") %>,
        "check": {
          "id": "redis-set",
          "name": "Redis set",
          "script": "redis-cli -p <%= p("redis.port") %> set consul-test \"$(date)\" || exit 2",
          "interval": "5s"
        }
    }
}
